{
    "name": "mobile-wikipedia-redilector",
    "version": "0.1.0",
    "description": "Redirects mobile wikipedia pages",
    "manifest_version": 2,
    "minimum_chrome_version": "73",
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "*://*.m.wikipedia.org/*"
    ],
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": true
    },
    "browser_specific_settings": {
        "gecko": {
            "id": "mobile-wikipedia-redilector@deadcode.rip",
            "strict_min_version": "52.0"
        }
    }
}
